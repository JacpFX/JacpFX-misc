<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<VBox HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:id="root">
    <children>
        <VBox VBox.vgrow="ALWAYS">
            <children>
                <VBox maxWidth="1.7976931348623157E308" fx:id="labelContainer" alignment="CENTER"
                      styleClass="propLabelBig">
                    <padding>
                        <Insets bottom="10.0" left="15.0" right="15.0" top="10.0"/>
                    </padding>
                    <children>
                        <Label text="%canvasName" VBox.vgrow="ALWAYS"/>
                    </children>
                </VBox>
                <Canvas fx:id="canvas" VBox.vgrow="ALWAYS"/>
            </children>
        </VBox>
    </children>
</VBox>
